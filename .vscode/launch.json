    "version": "0.2.0",
    "configurations": [


        {
            "name": "hannah.nas",
            "type": "python",
            "request": "launch",
            "module": "hannah.tools.train",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "nas=random_nas",
                "model=lazy_convnet",
                "module=image_classifier",
                "dataset=cifar10",
                "features=identity",
                "module.batch_size=32",
                "trainer.max_epochs=15",
                "nas.budget=3",
                "module.num_workers=8",
                "~normalizer"
            ]
        },
        {
            "name": "random_nas",
            "type": "python",
            "request": "launch",
            "module": "hannah.tools.train",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}/experiments/",
            "args": [
                "nas=random_nas",
                "model=lazy_convnet",
                "module=image_classifier",
                "dataset=cifar10",
                "features=identity",
                "module.batch_size=128",
                "trainer.max_epochs=1",
                "trainer.overfit_batches=20",
                "nas.budget=5",
                "module.num_workers=8",
                "nas.n_jobs=1",
                "experiment_id=random_nas_debug",
                // "~nas.predictor",
                "~normalizer"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },

        {
            "name": "standard",
            "type": "python",
            "request": "launch",
            "module": "hannah.tools.train",
            "args": [
                "trainer.limit_train_batches=1.0",
                "trainer.max_epochs=2",
                "model=conv-net-trax",
                "normalizer=fixedpoint",
                "compression=pruning_only"
            ]
        },

        {
            "name": "AgingEvolution",
            "type": "python",
            "request": "launch",
            "module": "hannah.tools.train",
            "justMyCode": false,
            "cwd": "${workspaceFolder}/experiments/",
            "args": [
                "nas=aging_evolution_nas",
                "model=lazy_convnet",
                "module=image_classifier",
                "dataset=cifar10",
                "features=identity",
                "module.batch_size=128",
                "trainer.max_epochs=1",
                "trainer.overfit_batches=20",
                "nas.n_jobs=1",
                "nas.budget=5",
                "module.num_workers=8",
                "experiment_id=aging_evolution_debug",
                // "~nas.predictor",
                "~normalizer"
            ]
        },
        {
            "name": "svd",
            "type": "python",
            "request": "launch",
            "module": "hannah.svd_train",
            "args": [
                "trainer.limit_train_batches=1.0",
                "trainer.max_epochs=10",
                "model=conv-net-trax",
                "svd_rank_compression=4",
                "output_dir=trained_models/svd_rank4"
            ]
        },
        {
            "name": "backend",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "args": [
                "trainer.max_epochs=1",
                "model=conv-net-trax",
                "clustering=true",
                "normalizer=fixedpoint",
                "backend=trax_ut",
                "backend.cols=8",
                "backend.standalone=True",
                "backend.rtl_simulation=False",
                "backend.synthesis=False",
                "backend.postsyn_simulation=False",
                "backend.power_estimation=False",
                "backend.num_inferences=1",
                "backend.period=1000",
                "backend.macro_type=RTL",
                "backend.backend_dir=/local/wernerju/tc-resnet8-accelerator",
                "backend.teda_dir=/local/wernerju/teda"
            ]
        },
        {
            "name": "quantized",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "args": [
                "trainer.limit_train_batches=0.1",
                "trainer.max_epochs=20",
                "model=conv-net-trax"
            ]
        },
        {
            "name": "ofa",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "args": [
                "--config-name",
                "nas_ofa",
                "trainer.gpus=[0]",
                "module.num_workers=8",
                "model=ofa"
            ]
        },
        {
          "name": "ofa dsc",
          "type": "python",
          "request": "launch",
          "module": "hannah.train",
          "args": [
              "--config-name",
              "nas_ofa",
              "trainer.limit_train_batches=0.1",
              "trainer.max_epochs=1",
              "module.num_workers=8",
              "model=ofa_group_dsc",
              "nas=ofa_nas_dsc_g_only",
              "experiment_id=dsc_test_normal",
          ]
      },
        {
          "name": "ofa_quant dsc",
          "type": "python",
          "request": "launch",
          "module": "hannah.train",
          "args": [
              "--config-name",
              "nas_ofa",
              "trainer.gpus=[1]",
              "module.num_workers=8",
              "model=ofa_dsc_quant",
              "nas=ofa_nas_dsc_long",
              "experiment_id=dsc_quant_long"
          ]
      },
        {
            "name": "ofa_quant",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "args": [
                "--config-name",
                "nas_ofa",
                "trainer.gpus=[0]",
                "module.num_workers=8",
                "model=ofa_quant"
            ]
        },
        {
            "name": "ofa_quant small",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "args": [
                "--config-name",
                "nas_ofa",
                "trainer.gpus=[0]",
                "trainer.limit_train_batches=0.1",
                "trainer.max_epochs=1",
                "module.num_workers=8",
                "model=ofa_group_quant",
                "nas=ofa_nas_group_test",
                "module.num_workers=8"
            ]
        },
        {
            "name": "multi_gpu",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "args": [
                "trainer.gpus=[0,1]",
                "trainer.limit_train_batches=0.1",
                "trainer.limit_val_batches=0.1",
                "trainer.limit_test_batches=0.1",
                "trainer.max_epochs=3",
                "model=tc-res8"
            ]
        },
        {
            "name": "kd",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "args": [
                "--config-name",
                "config_kd",
                "trainer.limit_train_batches=0.1",
                "trainer.max_epochs=3",
                "model=tc-res4"
            ]
        },
        {
            "name": "ofa",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "args": [
                "--config-name",
                "nas_ofa",
                "model=ofa_group_test",
                "nas=ofa_nas_group_test",
                "module.num_workers=8",
                "trainer.gpus=[1]",
            ]
        },
        {
            "name": "object_detection",
            "type": "python",
            "request": "launch",
            "module": "hannah.train",
            "justMyCode": false,
            "args": [
                "--config-name",
                "config_object_detection",
                "dataset.data_folder=/local/datasets"
            ]
        },
    ]
}
